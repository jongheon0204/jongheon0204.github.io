	<span class="p">}</span>
	<span class="kt">int</span> <span class="n">curY</span> <span class="o">=</span> <span class="n">point</span><span class="p">[</span><span class="n">depth</span><span class="p">].</span><span class="n">first</span><span class="p">;</span>
